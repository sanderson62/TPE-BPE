perl $HOME/bin/email_el551_csr.pl $SEQFILES/
!
EXECPGM pgmname='BPXBATCH' stepname='EMAILEL551' parm='SH'

################################################################################
LABEL name=PERL1
################################################################################
#
# This step reads the files from above and creates pdf files
#
ASSGNDD ddname='SYSIN' type='INSTREAM'  << !
$HOME/bin/elrpt2pdf $SEQFILES/FL.DL.CIADM.EL551.REPT $SEQFILES/FL.EL551.WHOLE.pdf
$HOME/bin/cidrpt2pdf $SEQFILES/FL.DL.RESEND.REPORT $SEQFILES/FNL_RESEND_REPORT.pdf
$HOME/bin/cidrpt2pdf $SEQFILES/FL.DL.FINALACT.REPORT $SEQFILES/FNL_FINALACT_REPORT.pdf
$HOME/bin/elrpt2pdf $SEQFILES/FL.DL.CIADM.FICH512.FINALACT $SEQFILES/FNL_FICH512_FINALACT.pdf
$HOME/bin/elrpt2pdf $SEQFILES/FL.DL.CIADM.FICH513.FINALACT $SEQFILES/FNL_FICH513_FINALACT.pdf
$HOME/bin/elrpt2pdf $SEQFILES/FL.DL.CIADM.FICH513A.FINALACT $SEQFILES/FNL_FICH513A_FINALACT.pdf
!
EXECPGM pgmname='BPXBATCH' stepname='PERL1' parm='SH'

if ($JOBSTATUS == 0) then

   ftp -n ntis2 < $JCLLIB/ftp-fllgcrltrs-ntis2
   ftp -n ntcso1 < $JCLLIB/ftp-fllgcrltrs
   ftp -n ntbatch2 < $JCLLIB/ftp-fllgcrltrs-ntbatch2

   "mutt -s 'EL551 Report Attached' -a $SEQFILES/FL.EL551.WHOLE.pdf pema@cso.com,jjva@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"
   "mutt -s 'FNL Resend Letter Report Attached' -a $SEQFILES/FNL_RESEND_REPORT.pdf dcpsnapersoftdaily@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"
   "mutt -s 'FNL Final Action Report Attached' -a $SEQFILES/FNL_FINALACT_REPORT.pdf dcpsnapersoftdaily@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"   
   "mutt -s 'FNL Final Action EL512 Report Attached' -a $SEQFILES/FNL_FICH512_FINALACT.pdf dcpsnapersoftdaily@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"   
   "mutt -s 'FNL Final Action EL513 Report Attached' -a $SEQFILES/FNL_FICH513_FINALACT.pdf dcpsnapersoftdaily@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"   
   "mutt -s 'FNL Final Action EL513A Report Attached' -a $SEQFILES/FNL_FICH513A_FINALACT.pdf dcpsnapersoftdaily@cso.com -e 'my_hdr FROM:slunikix<pema@cso.com>' < /dev/null"   

else
	echo " Job aborted, print not released "
endif

ENDJOB 
