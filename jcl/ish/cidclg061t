LABEL name=DELM35 
################################################################################
#  *****************************************************************     
#  ***  DELETE - - TEMP. FILES AT  E. O. J.                              
#  *****************************************************************     
ASSGNDD ddname='SYSPRINT' type='SYSOUT' class='M' 
ASSGNDD ddname='SYSIN' type='INSTREAM'  << !
