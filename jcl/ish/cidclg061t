 DELETE  CI.TT.LG.ERCOMP PURGE CLUSTER NOERASE                          
 DELETE  CI.TT.LG.ELCNTL PURGE CLUSTER NOERASE                          
 SET MAXCC = 00
!

EXECPGM pgmname='IDCAMS' stepname='DELM35' 

