01456         (RE-AH-CLM-PCT = ZERO) AND                                ECSRIRTN
01457         (RE-LF-CLM-PCT = ZERO) AND                                ECSRIRTN
01458         (RE-CLM-INCURRED-LIM = ZERO) AND                          ECSRIRTN
01459         (RE-EARNING-START-DT = ZERO) AND                          ECSRIRTN
01460         (RE-EARNING-STOP-DT = ZERO)                               ECSRIRTN
01461           GO TO REIN-CO-BUILD-EPR.                                ECSRIRTN
01462                                                                   ECSRIRTN
01463      ADD +1 TO CO-SUB.                                            ECSRIRTN
01464                                                                   ECSRIRTN
01465      IF CO-SUB GREATER REIN-CO-TABLE-ENT-CNT                      ECSRIRTN
01466         MOVE 0202 TO WS-RETURN-CODE                               ECSRIRTN
01467         DISPLAY 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'         ECSRIRTN
01468         DISPLAY 'ERROR DETECTED IN ECSRIRTN, SEE ECSRITAB'        ECSRIRTN
01469         MOVE 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'            ECSRIRTN
01470                                   TO WS-ABEND-MESSAGE             ECSRIRTN
01471         GO TO ABEND-PGM.                                          ECSRIRTN
01472                                                                   ECSRIRTN
01473      MOVE RE-COMPANY           TO RCT-REIN-CO (CO-SUB).              CL**7
01474                                                                   ECSRIRTN
01475      MOVE RE-CLM-INCURRED-LIM  TO RCT-CLM-CUTOFF-DT (CO-SUB)      ECSRIRTN
01476                                WS-RCT-CLM-CUTOFF-DT-N (CO-SUB).      CL*12
01477      MOVE RE-EARNING-START-DT  TO RCT-EARNING-START-DT (CO-SUB)   ECSRIRTN
01478                                WS-RCT-EARNING-START-DT-N (CO-SUB).   CL*12
01479      MOVE RE-EARNING-STOP-DT   TO RCT-EARNING-STOP-DT (CO-SUB)    ECSRIRTN
01480                                WS-RCT-EARNING-STOP-DT-N (CO-SUB).    CL*12
01481                                                                   ECSRIRTN
01482      IF RE-EARN-STOP-CODE = 'A'  OR  'L'                          ECSRIRTN
01483          MOVE RE-EARN-STOP-CODE  TO RCT-EARN-STOP-CODE (CO-SUB)   ECSRIRTN
01484      ELSE                                                         ECSRIRTN
01485          MOVE 'B'                TO RCT-EARN-STOP-CODE (CO-SUB).  ECSRIRTN
01486                                                                   ECSRIRTN
01487      MOVE RE-LF-CLM-PCT  TO RCT-LF-CLM-PCT (CO-SUB).              ECSRIRTN
01488      MOVE RE-AH-CLM-PCT  TO RCT-AH-CLM-PCT (CO-SUB).              ECSRIRTN
01489      MOVE RE-LF-CLM-MAX  TO RCT-LF-CLM-MAX (CO-SUB).              ECSRIRTN
01490      MOVE RE-AH-CLM-MAX  TO RCT-AH-CLM-MAX (CO-SUB).              ECSRIRTN
01491                                                                   ECSRIRTN
01492  REIN-CO-BUILD-EPR.                                               ECSRIRTN
01493                                                                   ECSRIRTN
01494      IF RE-AH-PR-PCT  NOT NUMERIC                                 ECSRIRTN
01495         MOVE ZERO TO RE-AH-PR-PCT.                                ECSRIRTN
01496                                                                   ECSRIRTN
01497      IF RE-AH-78-PCT  NOT NUMERIC                                 ECSRIRTN
01498         MOVE ZERO TO RE-AH-78-PCT.                                ECSRIRTN
01499                                                                   ECSRIRTN
01500      IF RE-LF-PR-PCT  NOT NUMERIC                                 ECSRIRTN
01501         MOVE ZERO TO RE-LF-PR-PCT.                                ECSRIRTN
01502                                                                   ECSRIRTN
01503      IF RE-LF-78-PCT  NOT NUMERIC                                 ECSRIRTN
01504         MOVE ZERO TO RE-LF-78-PCT.                                ECSRIRTN
01505                                                                   ECSRIRTN
01506      ADD +1 TO SUB1.                                              ECSRIRTN
01507                                                                   ECSRIRTN
01508      IF SUB1 GREATER REIN-CO-TABLE-ENT-CNT                        ECSRIRTN
01509         MOVE 0201 TO WS-RETURN-CODE                               ECSRIRTN
01510         MOVE 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'            ECSRIRTN
01511                                   TO WS-ABEND-MESSAGE             ECSRIRTN
01512         GO TO ABEND-PGM.                                          ECSRIRTN
01513                                                                   ECSRIRTN
01514      MOVE RE-COMPANY     TO RC2-REIN-CO    (SUB1).                ECSRIRTN
01515      MOVE RE-AH-PR-PCT   TO RC2-AH-PRO-PCT (SUB1).                ECSRIRTN
01516      MOVE RE-AH-78-PCT   TO RC2-AH-R78-PCT (SUB1).                ECSRIRTN
01517      MOVE RE-LF-PR-PCT   TO RC2-LF-PRO-PCT (SUB1).                ECSRIRTN
01518      MOVE RE-LF-78-PCT   TO RC2-LF-R78-PCT (SUB1).                ECSRIRTN
01519      MOVE RE-NAME        TO RC2-CO-NAME    (SUB1).
           move re-ah-pe       to rc2-ah-pe-method (sub1).
           if re-state-exhibit-opt-f = 'Y'
              move 'Y'         to rc2-ecs152-rpt (sub1)
           else
              move 'N'         to rc2-ecs152-rpt (sub1)
           end-if
01520                                                                   ECSRIRTN
01521      GO TO REIN-CO-BUILD-LOOP.                                    ECSRIRTN
01522                                                                   ECSRIRTN
01523  REIN-BUILD-EXIT.                                                 ECSRIRTN
01524      EXIT.                                                        ECSRIRTN
01525                                                                   ECSRIRTN
01526  ZERO-RCT-ENTRIES.                                                ECSRIRTN
01527                                                                   ECSRIRTN
01528      MOVE SPACES TO REIN-CO-HOLD-ENTRIES (CO-SUB).                ECSRIRTN
01529                                                                   ECSRIRTN
01530      MOVE ZERO   TO RCT-CLM-CUTOFF-DT (CO-SUB)                    ECSRIRTN
01531                     WS-RCT-CLM-CUTOFF-DT-N (CO-SUB)                  CL*12
