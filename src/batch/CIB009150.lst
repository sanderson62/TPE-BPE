* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   4
* CIB009150.cbl
   105/
   106 100-PROCESS.
   107*
   108     READ FICHE-FILE
   109          AT END GO TO 100-EXIT.
   110     ADD +1 TO IN-CNT
   111
   112     IF PRINT-CONTROL = '1'
   113        PERFORM 200-NEW-PAGE THRU 200-EXIT.
   114
   115     IF PRINT-SW = 'YES'
   116        EVALUATE PRINT-CONTROL
   117           WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   118                    ADD +1 TO OUT-CNT
   119           WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   120                    WRITE REPORT-RECORD FROM BLANK-LINE
   121                    ADD +2 TO OUT-CNT
   122        END-EVALUATE
   123        WRITE REPORT-RECORD FROM FICHE-RECORD
   124        ADD +1 TO OUT-CNT
   125     END-IF
   126     .
   127 100-EXIT.
   128     EXIT.
   129
   130
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   5
* CIB009150.cbl
   131/
   132 200-NEW-PAGE.
   133*
   134     IF PARM-LENGTH = +0
   135        MOVE 'YES' TO PRINT-SW
   136        GO TO 200-EXIT.
   137
   138     IF PARM-VALUE(1:PARM-LENGTH) = 'ALL'
   139        MOVE 'YES' TO PRINT-SW
   140        GO TO 200-EXIT.
   141
   142     MOVE 'NO' TO PRINT-SW
   143
   144     MOVE SPACES     TO WORKAREA
   145     MOVE FICHE-REC  TO SAVE-REC(1)
   146     PERFORM VARYING SUB FROM 2 BY 1
   147       UNTIL SUB > 10 OR EOF
   148        READ FICHE-FILE INTO SAVE-REC(SUB)
   149     END-PERFORM
   150
   151     PERFORM VARYING STRT FROM 1 BY 1
   152       UNTIL PRINT-SW = 'YES' OR STRT > END-LINE
   153          IF WORKAREA(STRT:PARM-LENGTH) =
   154             PARM-VALUE(1:PARM-LENGTH)
   155                MOVE 'YES' TO PRINT-SW
   156          END-IF
   157     END-PERFORM
   158
   159     IF PRINT-SW = 'YES'
   160        PERFORM VARYING SUB FROM 1 BY 1 UNTIL SUB > 9
   161          EVALUATE SAVE-REC(SUB)(1:1)
   162            WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   163                     ADD +1 TO OUT-CNT
   164            WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   165                     WRITE REPORT-RECORD FROM BLANK-LINE
   166                     ADD +2 TO OUT-CNT
   167          END-EVALUATE
   168          WRITE REPORT-RECORD FROM SAVE-REC(SUB)(2:149)
   169          ADD +1 TO OUT-CNT
   170        END-PERFORM
   171     END-IF
   172     .
   173 200-EXIT.
   174     EXIT.
   175
   176 900-END-PROGRAM.
   177
   178     CLOSE REPORT-FILE
   179     DISPLAY 'RECORDS IN....' IN-CNT
   180     DISPLAY 'RECORDS OUT...' OUT-CNT
   181
   182     .
   183 900-EXIT.
   184     EXIT.
