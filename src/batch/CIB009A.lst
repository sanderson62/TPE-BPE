* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   5
* CIB009A.cbl
    90/
    91 000-INIT.
    92*
    93     OPEN INPUT FICHE-FILE
    94     IF SYS010-STATUS NOT = '00'
    95        DISPLAY 'OPEN ERROR ' SYS010-STATUS ' ON SYS010'
    96        MOVE '10' TO SYS010-STATUS
    97     END-IF
    98
    99     OPEN OUTPUT REPORT-FILE
   100
   101     COMPUTE END-LINE = LENGTH OF WORKAREA - PARM-LENGTH
   102     if parm-length > +0
   103        inspect parm-value replacing
   104           all '-' by ' '
   105     end-if
   106
   107     .
   108 000-EXIT.
   109     EXIT.
   110
   111
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   6
* CIB009A.cbl
   112/
   113 100-PROCESS.
   114*
   115     READ FICHE-FILE
   116          AT END GO TO 100-EXIT.
   117     ADD +1 TO IN-CNT
   118
   119     IF PRINT-CONTROL = '1'
   120        PERFORM 200-NEW-PAGE THRU 200-EXIT.
   121
   122     IF PRINT-SW = 'YES'
   123        EVALUATE PRINT-CONTROL
   124           WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   125                    ADD +1 TO OUT-CNT
   126           WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   127                    WRITE REPORT-RECORD FROM BLANK-LINE
   128                    ADD +2 TO OUT-CNT
   129        END-EVALUATE
   130        WRITE REPORT-RECORD FROM FICHE-RECORD
   131        ADD +1 TO OUT-CNT
   132     END-IF
   133     .
   134 100-EXIT.
   135     EXIT.
   136
   137
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   7
* CIB009A.cbl
   138/
   139 200-NEW-PAGE.
   140*
   141     IF PARM-LENGTH = +0
   142        MOVE 'YES' TO PRINT-SW
   143        GO TO 200-EXIT.
   144
   145     IF PARM-VALUE(1:PARM-LENGTH) = 'ALL'
   146        MOVE 'YES' TO PRINT-SW
   147        GO TO 200-EXIT.
   148
   149     MOVE 'NO' TO PRINT-SW
   150
   151     MOVE SPACES     TO WORKAREA
   152     MOVE FICHE-REC  TO SAVE-REC(1)
   153     PERFORM VARYING SUB FROM 2 BY 1
   154       UNTIL SUB > 10 OR EOF
   155        READ FICHE-FILE INTO SAVE-REC(SUB)
   156     END-PERFORM
   157
   158     PERFORM VARYING STRT FROM 1 BY 1
   159       UNTIL PRINT-SW = 'YES' OR STRT > END-LINE
   160          IF WORKAREA(STRT:PARM-LENGTH) =
   161             PARM-VALUE(1:PARM-LENGTH)
   162                MOVE 'YES' TO PRINT-SW
   163          END-IF
   164     END-PERFORM
   165
   166     IF PRINT-SW = 'YES'
   167        PERFORM VARYING SUB FROM 1 BY 1 UNTIL SUB > 9
   168          EVALUATE SAVE-REC(SUB)(1:1)
   169            WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   170                     ADD +1 TO OUT-CNT
   171            WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   172                     WRITE REPORT-RECORD FROM BLANK-LINE
   173                     ADD +2 TO OUT-CNT
   174          END-EVALUATE
   175          WRITE REPORT-RECORD FROM SAVE-REC(SUB)(2:132)
   176          ADD +1 TO OUT-CNT
   177        END-PERFORM
   178     END-IF
   179     .
   180 200-EXIT.
   181     EXIT.
   182
   183
