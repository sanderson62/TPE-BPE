* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   5
* CIB009.cbl
    89/
    90 000-INIT.
    91*
    92     OPEN INPUT FICHE-FILE
    93     IF SYS010-STATUS NOT = '00'
    94        DISPLAY 'OPEN ERROR ' SYS010-STATUS ' ON SYS010'
    95        MOVE '10' TO SYS010-STATUS
    96     END-IF
    97
    98     OPEN OUTPUT REPORT-FILE
    99
   100     COMPUTE END-LINE = LENGTH OF WORKAREA - PARM-LENGTH
   101     .
   102 000-EXIT.
   103     EXIT.
   104
   105
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   6
* CIB009.cbl
   106/
   107 100-PROCESS.
   108*
   109     READ FICHE-FILE
   110          AT END GO TO 100-EXIT.
   111     ADD +1 TO IN-CNT
   112
   113     IF PRINT-CONTROL = '1'
   114        PERFORM 200-NEW-PAGE THRU 200-EXIT.
   115
   116     IF PRINT-SW = 'YES'
   117        EVALUATE PRINT-CONTROL
   118           WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   119                    ADD +1 TO OUT-CNT
   120           WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   121                    WRITE REPORT-RECORD FROM BLANK-LINE
   122                    ADD +2 TO OUT-CNT
   123        END-EVALUATE
   124        WRITE REPORT-RECORD FROM FICHE-RECORD
   125        ADD +1 TO OUT-CNT
   126     END-IF
   127     .
   128 100-EXIT.
   129     EXIT.
   130
   131
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   7
* CIB009.cbl
   132/
   133 200-NEW-PAGE.
   134*
   135     IF PARM-LENGTH = +0
   136        MOVE 'YES' TO PRINT-SW
   137        GO TO 200-EXIT.
   138
   139     IF PARM-VALUE(1:PARM-LENGTH) = 'ALL'
   140        MOVE 'YES' TO PRINT-SW
   141        GO TO 200-EXIT.
   142
   143     MOVE 'NO' TO PRINT-SW
   144
   145     MOVE SPACES     TO WORKAREA
   146     MOVE FICHE-REC  TO SAVE-REC(1)
   147     PERFORM VARYING SUB FROM 2 BY 1
   148       UNTIL SUB > 10 OR EOF
   149        READ FICHE-FILE INTO SAVE-REC(SUB)
   150     END-PERFORM
   151
   152     PERFORM VARYING STRT FROM 1 BY 1
   153       UNTIL PRINT-SW = 'YES' OR STRT > END-LINE
   154          IF WORKAREA(STRT:PARM-LENGTH) =
   155             PARM-VALUE(1:PARM-LENGTH)
   156                MOVE 'YES' TO PRINT-SW
   157          END-IF
   158     END-PERFORM
   159
   160     IF PRINT-SW = 'YES'
   161        PERFORM VARYING SUB FROM 1 BY 1 UNTIL SUB > 9
   162          EVALUATE SAVE-REC(SUB)(1:1)
   163            WHEN '0' WRITE REPORT-RECORD FROM BLANK-LINE
   164                     ADD +1 TO OUT-CNT
   165            WHEN '-' WRITE REPORT-RECORD FROM BLANK-LINE
   166                     WRITE REPORT-RECORD FROM BLANK-LINE
   167                     ADD +2 TO OUT-CNT
   168          END-EVALUATE
   169          WRITE REPORT-RECORD FROM SAVE-REC(SUB)(2:132)
   170          ADD +1 TO OUT-CNT
   171        END-PERFORM
   172     END-IF
   173     .
   174 200-EXIT.
   175     EXIT.
   176
   177
