* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   5
* CIB009C.cbl
    89/
    90 000-INIT.
    91*
    92     OPEN INPUT FICHE-FILE
    93     IF SYS010-STATUS NOT = '00'
    94        DISPLAY 'OPEN ERROR ' SYS010-STATUS ' ON SYS010'
    95        MOVE '10' TO SYS010-STATUS
    96     END-IF
    97
    98     OPEN OUTPUT REPORT-FILE
    99
   100     COMPUTE END-LINE = LENGTH OF WORKAREA - PARM-LENGTH
   101     if parm-length > +0
   102        inspect parm-value replacing
   103           all '-' by ' '
   104     end-if
   105
   106     .
   107 000-EXIT.
   108     EXIT.
   109
   110 100-PROCESS.
   111*
   112     READ FICHE-FILE
   113          AT END GO TO 100-EXIT.
   114     ADD +1 TO IN-CNT
   115
   116     IF PRINT-CONTROL = '1'
   117        PERFORM 200-NEW-PAGE THRU 200-EXIT.
   118
   119     IF PRINT-SW = 'YES'
   120        WRITE REPORT-RECORD FROM FICHE-REC
   121        ADD +1 TO OUT-CNT
   122     END-IF
   123     .
   124 100-EXIT.
   125     EXIT.
   126
   127
   128 200-NEW-PAGE.
   129*
   130     IF PARM-LENGTH = +0
   131        MOVE 'YES' TO PRINT-SW
   132        GO TO 200-EXIT.
   133
   134     IF PARM-VALUE(1:PARM-LENGTH) = 'ALL'
   135        MOVE 'YES' TO PRINT-SW
   136        GO TO 200-EXIT.
   137
   138     MOVE 'NO' TO PRINT-SW
   139
   140     MOVE SPACES     TO WORKAREA
   141     MOVE FICHE-REC  TO SAVE-REC(1)
   142     PERFORM VARYING SUB FROM 2 BY 1
   143       UNTIL SUB > 10 OR EOF
   144        READ FICHE-FILE INTO SAVE-REC(SUB)
   145     END-PERFORM
   146
* Micro Focus Server Express         V5.1 revision 000 06-Oct-15 17:18 Page   6
* CIB009C.cbl
   147     PERFORM VARYING STRT FROM 1 BY 1
   148       UNTIL PRINT-SW = 'YES' OR STRT > END-LINE
   149          IF WORKAREA(STRT:PARM-LENGTH) =
   150             PARM-VALUE(1:PARM-LENGTH)
   151                MOVE 'YES' TO PRINT-SW
   152          END-IF
   153     END-PERFORM
   154
   155     IF PRINT-SW = 'YES'
   156        PERFORM VARYING SUB FROM 1 BY 1 UNTIL SUB > 9
   157          WRITE REPORT-RECORD FROM SAVE-REC(SUB)
   158          ADD +1 TO OUT-CNT
   159        END-PERFORM
   160     END-IF
   161     .
   162 200-EXIT.
   163     EXIT.
   164
